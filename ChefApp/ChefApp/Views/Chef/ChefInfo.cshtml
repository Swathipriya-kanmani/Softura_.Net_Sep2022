@model IEnumerable<ChefApp.Models.ChefDetails>
<table class="table" border="1">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(Model => Model.Id)</th>
            <th>@Html.DisplayNameFor(Model => Model.Name)</th>
            <th>@Html.DisplayNameFor(Model => Model.Dishes)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var c in Model)
        {
            <tr>
                <td>@c.Id</td>
                <td>@c.Name</td>
                <td>@c.Dishes</td>
            </tr>
        }
    </tbody>
</table>






